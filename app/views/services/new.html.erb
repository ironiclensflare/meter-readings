<h1>Create a new service</h1>
<%= form_for :service, url: services_path do |f| %>
<div class="form-group">
<%= f.label :name %>
<%= f.text_field :name, class: 'form-control' %>
</div>
<div class="form-group">
<%= f.label :icon %>
<%= f.text_field :icon, class: 'form-control' %>
</div>
<div class="form-group">
<%= f.submit class: 'btn btn-primary' %>
</div>
<% end %>